[
  {
    "name": "Pausing the play for 40seconds to fully up the service",
    "pause": {
      "seconds": 40
    }
  },
  {
    "name": "checking leader",
    "shell": "echo srvr | nc localhost 2181",
    "register": "output"
  },
  {
    "debug": {
      "var": "output"
    },
    "when": "\"leader\" in output.stdout"
  }
]
